{
    "name": "justraviga/laravel-pexels",
    "description": "Pexels API client for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "pexels",
        "pexels api",
        "api client"
    ],
    "authors": [
        {
            "name": "Hodun Dmytro",
            "email": "godundmytro@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/JustRaviga/laravel-pexels/issues",
        "source": "https://github.com/JustRaviga/laravel-pexels"
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "guzzlehttp/guzzle": "^6.2.1|^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "JustRaviga\\Pexels\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "JustRaviga\\Pexels\\PexelsServiceProvider"
            ],
            "aliases": {
                "Pexels": "JustRaviga\\Pexels\\Facades\\Pexels"
            }
        }
    }
}
